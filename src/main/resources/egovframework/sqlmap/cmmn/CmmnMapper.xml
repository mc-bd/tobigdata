<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alticast.allso.cmmn.service.impl.CmmnMapper">

	<select id="selectCmmnCode" parameterType="Map" resultType="egovMap">
			SELECT
				*
			FROM cmmn_code_sub
			WHERE CMMN_CODE1 = #{cmmnCode1}
			ORDER BY ORDER_NUM ASC
	</select>

</mapper>